{
    "name": "kmeal-feeder",
    "version": "1.0.0",
    "description": "Feeder from EOSWS to RabbitMQ",
    "main": "dist/server.js",
    "scripts": {
        "build": "rimraf ./dist && tsc",
        "postinstall": "npm run build",
        "deploy": "npm run build && serverless deploy",
        "start": "nodemon --exec ts-node ./src/index.ts"
    },
    "author": "",
    "license": "ISC",
    "engines": {
        "node": "8.11.4"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
    "devDependencies": {
        "@types/jest": "^23.3.5",
        "jest": "^23.6.0",
        "prettier": "^1.14.3",
        "supertest": "^3.3.0",
        "ts-jest": "^23.10.4",
        "tslint-config-prettier": "^1.15.0",
        "tslint-plugin-prettier": "^1.3.0",
        "@types/node": "^10.12.0",
        "@types/ws": "^6.0.1",
        "nodemon": "^1.18.9",
        "ts-node": "^7.0.1",
        "tslint": "^5.11.0",
        "typescript": "^3.2.2"
    },
    "dependencies": {
        "@dfuse/eosws-js": "^0.11.5",
        "@types/bluebird": "^3.5.24",
        "amqp-ts": "^1.4.0",
        "amqplib": "^0.5.2",
        "dotenv": "^6.1.0"
    }
}

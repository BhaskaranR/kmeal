    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "build-prod.web.kmeal": "ng build --prod web-kmeal",
    "build-prod.web-restaurant": "ng build --prod web-kmeal-restaurant",
    "deploy.web.kmeal.prod": "ng build --prod web-kmeal && rm -rf firebase.json && cp firebase.kmeal.json firebase.json || true && firebase use web.kmeal-prod && firebase deploy || true",
    "deploy.web.kmeal-restaurant.prod": "ng build --prod web-kmeal-restaurant && rm -rf firebase.json && cp firebase.kmeal-restaurant.json firebase.json || true && firebase use web.kmeal-restaurant-prod && firebase deploy || true",
    "gen-apollo-client": "gql-gen --config ./scripts/codegen-client.js",
    "gen-apollo-restaurant": "node ./scripts/codegen-restaurant.js",
    "postinstall": "node patch.js"
  },

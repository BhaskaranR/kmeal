{
  "compileOnSave": false,
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom",
      "esnext.asynciterable"
    ],
    "baseUrl": ".",
    "paths": {
      "*": ["types/*"],
      "@kmeal-env/*": ["apps/web-kmeal/src/environments/kmeal/*"],
      "@kmeal-restaurant-env/*": ["apps/web-kmeal-restaurant/src/environments/restaurant/*"],
      "@env/frontend": ["environments/kmeal/environment.ts"],
      "@env/restaurant": ["environments/restaurant/environment.ts"],
      "@utils": ["libs/utils"],
      "@kmeal-nx/feature-order": [
        "libs/feature-order/src/index.ts"
      ],
      "@kmeal-nx/ui": [
        "libs/ui/src/index.ts"
      ],
      "@kmeal-nx/feature-home": [
        "libs/feature-home/src/index.ts"
      ],
      "@kmeal-nx/feature-profile": [
        "libs/feature-profile/src/index.ts"
      ],
      "@kmeal-nx/feature-settings": [
        "libs/feature-settings/src/index.ts"
      ],
      "@kmeal-nx/feature-menu": [
        "libs/feature-menu/src/index.ts"
      ],
      "@kmeal-nx/feature-restaurant-orders": [
        "libs/feature-restaurant-orders/src/index.ts"
      ],
      "@kmeal-nx/feature-restaurant-dashboard": [
        "libs/feature-restaurant-dashboard/src/index.ts"
      ],
      "@kmeal-nx/feature-restaurant-menu": [
        "libs/feature-restaurant-menu/src/index.ts"
      ],
      "@kmeal-nx/feature-restaurant-profile": [
        "libs/feature-restaurant-profile/src/index.ts"
      ],
      "@kmeal-nx/feature-restaurant": [
        "libs/feature-restaurant/src/index.ts"
      ],
      "@kmeal-nx/feature-navigation-bar": [
        "libs/feature-navigation-bar/src/index.ts"
      ],
      "@kmeal-nx/feature-search": [
        "libs/feature-search/src/index.ts"
      ],
      "jszip": [
        "./node_modules/jszip/dist/jszip.min.js"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
